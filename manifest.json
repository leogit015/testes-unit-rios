{
  "manifest_version": 3,
  "name": "Testes Unitários",
  "version": "1.0",
  "description": "Automação de testes unitários.",
  "permissions": ["activeTab", "scripting", "storage", "downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}